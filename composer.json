{
  "name": "dennisdigital/behat-field-aliases",
  "type": "behat-extension",
  "description": "Provides support to use friendly field names that are human readable",
  "keywords": ["behat", "content", "entities"],
  "homepage": "https://github.com/dennisinteractive/behat-field-aliases",
  "license": "MIT",
  "require": {
    "drupal/drupal-extension": "~3.0",
    "drupal/drupal-driver": "~1.0",
    "behat/mink": "^1.7"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0"
  },
  "autoload": {
    "files": [
      "src/Context/FieldAliasesClass/FieldAliases.php"
    ],
    "psr-0": {
      "Drupal\\FieldAliases": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master-dev"
    }
  }
}
